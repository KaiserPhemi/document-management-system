{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A full-stack document management system",
  "main": "client/Index.jsx",
  "scripts": {
    "start": "npm run server",
    "start:dev": "NODE_ENV=development npm run server",
    "cover": "NODE_ENV=test node_modules/babel-cli/bin/babel-node.js node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- server/tests/**/**/*.js",
    "test:client": "NODE_ENV=test mocha --verbose --watch --coverage --reporter progress TestSetup.js server/tests/client/**/*.test.js",
    "test:server": "NODE_ENV=test mocha --compilers js:babel-core/register server/tests/server/**/*.js",
    "babel": "babel-cli",
    "dev:server": "nodemon --watch server ./node_modules/.bin/babel-node  -- server/Server.js",
    "server": "./node_modules/.bin/babel-node -- server/Server.js",
    "webpack": "webpack -p babel-node",
    "jsdoc": "jsdoc -c conf.json",
    "lint:client": "eslint client/",
    "lint:server": "eslint server/",
    "linting": "npm-run-all lint:server lint:client",
    "main-server-test": "npm run 'drop:db' && npm run 'create:db' && npm run 'migrate:db' && npm run test:server",
    "build": "webpack --config ./webpack.config.js --progress",
    "webpack-dev-server": "webpack-dev-server --progress --inline --open",
    "coverage": "`npm bin`/jasmine-node cover server/tests/**/**/*.js",
    "drop:db": "psql -c 'drop database if exists doqman_test;' -U postgres",
    "create:db": "psql -c 'create database doqman_test;' -U postgres",
    "migrate:db": "NODE_ENV=test sequelize db:migrate",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "server:test": "NODE_ENV=test mocha --compilers js:babel-core/register server/tests/server/**/*.js"
  },
  "author": "Oluwafemi Akinwa",
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-oakinwa/document-management-system"
  },
  "license": "MIT",
