{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A full-stack document management system",
  "main": "client/index.js",
  "scripts": {
    "start": "npm-run-all clean webpack",
    "start:dev": "npm-run-all clean webpack-dev-server",
    "clean-dist": "rimraf ./client/public/dist && mkdir client/public/dist",
    "clean-build": "rimraf ./client/public/build && mkdir client/public/build",
    "clean": "npm-run-all clean-dist clean-build",
    "test": "jasmine-node server/tests/**/*.js",
    "babel": "babel",
    "server": "nodemon server/Server.js",
    "webpack": "webpack -p --watch --progress --colors",
    "webpack-dev-server": "webpack-dev-server --progress --colors --inline --watch",
    "coverage": "`npm bin`/jasmine-node cover server/tests/**/*.js",
    "coveralls": "istanbul cover jasmine-node --captureExceptions server/tests/**/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "author": "Oluwafemi Akinwa",
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-oakinwa/document-management-system"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "fs": "0.0.1-security",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.6.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-verbose-reporter": "0.0.6",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jade": "^1.11.0",
    "js-logger": "^1.3.0",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0"
  }
}
