{
  "src_folders": ["e2e"],
  "output_folder": "e2eResult/reports",
  "custom_commands_path": "",
  "custom_assertions_path": "",
  "page_objects_path": "",
  "globals_path": "",

  "selenium": {
    "start_process": true,
    "server_path": "./e2e/bin/selenium-server-standalone-3.4.0.jar",
    "log_path": "e2eResult/reports",
    "host": "127.0.0.1",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "./e2e/bin/chromedriver"
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "https://localhost",
      "selenium_port": 4444,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "path": "e2eResult/screenshots"
      },
      "globals": {
        "waitForConditionTimeout": 5000
      },
      "desiredCapabilities": {
        "chromeOptions": {
          "args": ["window-size=2560,1600"]
        },
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      }
    }
  }
}